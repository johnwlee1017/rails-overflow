<h1>Rails-Overflow</h1>

<%= link_to('Create New Question', new_question_path) %>

<h2>Questions:</h2>
<ul>
  <% @questions.each do |question| %>
    <li><%= link_to(question.title, question) %> - <%= link_to('Edit', edit_question_path(question.id)) %> | <%= link_to('Delete', question_path(question), method: :delete) %></li>
  <% end %>
</ul>

